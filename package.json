{
  "name": "cli-path",
  "version": "0.1.11",
  "description": "Answer to the simple question: Should I stay or should I go?",
  "author": "iosifv",
  "license": "MIT",
  "keywords": [
    "cli"
  ],
  "main": "index.js",
  "type": "module",
  "bin": {
    "clip": "./bin/clip.js"
  },
  "scripts": {
    "configstore-watch": "watch cat ~/.config/configstore/cli-path.json",
    "configstore-edit": "code ~/.config/configstore/cli-path.json",
    "set-executable": "chmod 777 ./bin/clip.js && ls ./bin -lisa",
    "npm-publish": "git add . && git commit -m 'publishing new version' && npm version patch &&  git push && npm publish",
    "start": "node ./bin/clip.js"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "chalk": "^5.1.2",
    "commander": "^9.4.1",
    "configstore": "^6.0.0",
    "inquirer": "^9.1.4",
    "underscore": "^1.13.6"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  }
}
